<%= form_tag("/shipmentreceive") do %>

 <% @receiving.each_with_index do |receive, index| %>	
 	<% if index <= @sequence %>			
		<%= editable_input(receive)%>
	<% end %>	
 <% end %>


<div>
  <%= content_tag(:label, "Error: " +  @response["errors"][0]["message"].to_s) if  @response["status"] != '200'%>
</div>  

<div>
  <%= submit_tag("Receive") %>
</div>  
</div>

<% end %>