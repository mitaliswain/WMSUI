wms.controller('MenuCtrl', function ($scope, $http,  $location, $localStorage, $filter, $q, Auth) {
	if (Auth.getTokenClaims()) 
        $scope.user_name = Auth.getTokenClaims().user_id
 	
 	$scope.signout = function() {
 		delete $localStorage.token;
 		window.location = '/login'
 	}
   
        
});