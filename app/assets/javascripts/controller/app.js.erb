var shipment = angular.module('shipment_detail', ["ngRoute", "xeditable", "ngGrid"]);
var configuration = angular.module('configuration_detail', ["ngRoute", "xeditable", "ngGrid"]);

shipment.config(['$routeProvider',
 function($routeProvider) {
   $routeProvider.
     when('/shipmentHeader', {
       templateUrl: 'shipment_header.html', controller: 'shipmentEditCtrl'})      
    .otherwise({
            templateUrl: '/assets/shipment_header.html', controller: 'shipmentEditCtrl'});
     
}]);

shipment.run(function(editableOptions) {
 editableOptions.theme = 'bs3';
});
 