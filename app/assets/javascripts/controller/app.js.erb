var shipment = angular.module('shipment_detail', ["ngRoute", "xeditable"]);
var case_data = angular.module('case_detail', ["ngRoute", "xeditable"]);
var configuration = angular.module('configuration_detail', ["ngRoute", "xeditable"]);

shipment.config(['$routeProvider',
 function($routeProvider) {
   $routeProvider.
     when('/shipmentHeader', {
       templateUrl: 'shipment_header.html', controller: 'shipmentEditCtrl'})
    .otherwise({
            templateUrl: '/assets/shipment_maintenance/shipment_header.html', controller: 'shipmentEditCtrl'});
     
}]);

shipment.run(function(editableOptions) {
 editableOptions.theme = 'bs3';
});
case_data.run(function(editableOptions) {
 editableOptions.theme = 'bs3';
});
 
 