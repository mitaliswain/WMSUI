wms.controller('ItemMasterEditCtrl', function ($scope, $modalInstance, item, $http, $location, $q) {
  var url = '/item_master_maintenance/'+  item + '.json?client=' + 'WM' ;



  $http.get(url).success(function(data) {
    $scope.item_header = data.item_header;
  });

    $scope.statuses = [
        {value: 'Active', text: 'Active'},
        {value: 'Inactive', text: 'Inactive'}
    ];

    $scope.concept = [
        {value: 'AB', text: 'AB'},
        {value: 'BC', text: 'BC'},
    ];
    $scope.header_template = {
        name: 'header_template',
        url: '<%= asset_path('item_master_maintenance/item_header.html') %>'
        };


    $scope.updateHeader = function(data, el, id) {
        var url = '/item_master_maintenance/'+ id
        UserService.updateResource(data, el, id, url, $scope, $q)
    };

    $scope.ok = function () {
        $modalInstance.close();
    };

    $scope.cancel = function () {
        $modalInstance.dismiss('cancel');
    };

});
